{
  "manifest_version": 2,
  "name": "To-Do List",
  "description": "Allows the user to create a To-Do List",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "window.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["todo.js"]
    }]
}
